@model IEnumerable<Pemesanan>
@{
    Layout = "_LayoutAkun";
}

<h4 class="text-center">Isi Data</h4>
<hr />
<br />


<div class="row">
    <div class="col-md-12">
        <form asp-action="Create" method="post">
           
            <div class="form-group">
                <label  class="control-label"></label>
                <input  placeholder="Masukan nama pelanggan" class="form-control" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <label  class="control-label"></label>
                <input  placeholder="Masukan jenis kelamin" class="form-control" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <label  class="control-label"></label>
                <input  placeholder="Masukan daerahb asal" class="form-control" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <label  class="control-label"></label>
                <input  class="form-control" placeholder="Masukan NO ktp" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <label  class="control-label"></label>
                <input  class="form-control" placeholder="Masukan NO telp" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <label  class="control-label"></label>
                <input  class="form-control" placeholder="Masukan type tiket" />
                <span  class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Lanjut" class="btn btn-primary" />
                <input asp-controller="TrBus" asp-action="Index" type="submit" value="Kembali" class="btn btn-primary float-xl-right" />
            </div>
        </form>
    </div>
</div>